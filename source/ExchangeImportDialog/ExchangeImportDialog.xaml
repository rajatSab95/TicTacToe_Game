<Window x:Class="hps_wpf_sandbox.ExchangeImportDialog.ExchangeImportDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ExchangeImportDialog" Height="323.893" Width="459.292">
    <Grid Height="285" VerticalAlignment="Top">
        <ProgressBar x:Name="m_progress_bar" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" VerticalAlignment="Top" Width="431" IsIndeterminate="True"/>
        <Label x:Name="m_import_message" Content="Import Message" Margin="10,38,0,0" VerticalAlignment="Top" HorizontalAlignment="Center"/>
        <TextBox x:Name="m_import_log" AcceptsReturn="True" HorizontalAlignment="Left" Height="170" Margin="10,69,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="431" UndoLimit="0" AllowDrop="False" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" IsReadOnly="True" IsUndoEnabled="False"/>
        <CheckBox x:Name="m_keep_open_check" Content="Keep Dialog Open" HorizontalAlignment="Left" Margin="10,256,0,0" VerticalAlignment="Top" Height="19" Width="208" Checked="OnKeepOpenChecked"/>
        <Button x:Name="m_cancel_button" Content="Cancel Button" HorizontalAlignment="Left" Margin="239,256,0,0" VerticalAlignment="Top" Width="202" Click="OnCancelClicked"/>

    </Grid>
</Window>
